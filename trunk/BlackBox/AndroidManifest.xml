<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="nutes.intelimed"
      android:versionCode="1"
      android:versionName="1.0">

    <uses-sdk android:minSdkVersion="4" />
    
    <uses-permission android:name="android.permission.INTERNET">
   </uses-permission>
   
    <application android:icon="@drawable/logo" android:label="@string/app_name">
    
    	<activity android:name=".activity.Login" android:label="@string/app_name" android:configChanges="keyboardHidden|orientation">
           	<intent-filter>
	              <action android:name="android.intent.action.MAIN" />
	              <category android:name="android.intent.category.LAUNCHER" />
         	</intent-filter>
        </activity>
        <activity android:name=".controller.ReceiveTree" android:label="@string/app_name" android:configChanges="keyboardHidden|orientation" android:screenOrientation="portrait"/>
                  
        <activity android:name=".activity.FormDiagnostic" android:label="@string/app_name" android:configChanges="keyboardHidden|orientation" android:screenOrientation="portrait" />

        <activity android:name=".activity.ResultDiagnostic" android:label="@string/app_name" android:configChanges="keyboardHidden|orientation" android:screenOrientation="portrait"/>
        
        <activity android:name=".activity.Menu" android:label="@string/app_name" android:configChanges="keyboardHidden|orientation" android:screenOrientation="portrait">
		<intent-filter>
	              <action android:name="android.intent.action.MAIN" />
         	</intent-filter>
         </activity>
		 
    </application>
</manifest>